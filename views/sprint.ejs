<link rel="stylesheet" href="css/sprint.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script> 
<table>
        <thead>
            <tr>
                <th>Story</th>
                <th>Duedate</th>
                <th>Progress (%)</th>
                <th>Sub-Tasks</th>
                <th>Timespent (hours)</th>
            </tr>
        </thead>
        <tbody>
            <% for (var i=0; i<keys.length;i++) {%>
                <tbody class="labels">
                    <tr>
                        <td colspan="5">
                            <label for="<%=keys[i]%>"><%=keys[i]%> </label>
                            <input type="checkbox" name="<%=keys[i]%>" id="<%=keys[i]%>" data-toggle="toggle">
                        </td>
                    </tr>
                </tbody>
                <tbody class="hide" style="display: none;">
                    <% for (var j=0; j<finish[keys[i]].length;j++) {%>
                        <tr>
                            <td><a href="https://issues.qup.vn/browse/<%=finish[keys[i]][j].key%>"><%=finish[keys[i]][j].key%></a> - <%=finish[keys[i]][j].summary%></td>
                            <td><%=finish[keys[i]][j].duedate%></td>
                            <td><%=finish[keys[i]][j].aggregateprogress.percent%></td>
                            <td><%=finish[keys[i]][j].subtasks.length%></td>
                            <td><%=finish[keys[i]][j].aggregateprogress.progress/60/60%></td>
                        </tr>
                    <% } %>
                </tbody>
            <% } %>
            
        </tbody>
    </table>
<script>
$(document).ready(function() {
    $('[data-toggle="toggle"]').change(function(){
        $(this).parents().next('.hide').toggle();
    });
});
</script>